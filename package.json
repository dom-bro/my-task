{
  "name": "vue-study",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:modern": "vue-cli-service build --modern",
    "lint": "lint-staged"
  },
  "dependencies": {
    "element-ui": "^2.4.4",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/eslint-config-standard": "^3.0.0-rc.3",
    "axios": "^0.18.0",
    "colors": "^1.3.0",
    "eslint": "^5.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.7.0",
    "husky": "^1.0.0-rc.13",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "lint-staged": "^7.2.0",
    "lowdb": "^1.0.0",
    "node-sass": "^4.9.2",
    "prettier": "^1.13.7",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./githooks/commit-msg"
    }
  },
  "lint-staged": {
    "*.{json,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "vue-cli-service lint",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
